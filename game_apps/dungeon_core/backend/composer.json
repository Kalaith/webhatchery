{
    "name": "dungeon-core/backend",
    "description": "Dungeon Core Game Backend API",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "slim/slim": "^4.12",
        "slim/psr7": "^1.6",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "DungeonCore\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}